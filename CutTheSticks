namespace getTotalX
{
  public class CutTheSticks
  {
    public static List<int> cutTheSticks(List<int> arr)
    {
        List<int> result = new List<int>();
        arr.Sort();
        int min = arr[0];
        result.Add(arr.Count());
        for(int i=0;i<arr.Count();i++)
        {
            if(arr[i] > min )
            {
                min = arr[i];
                result.Add(arr.Count()-i);
            }
        }
        return result;
    }
  }
}
